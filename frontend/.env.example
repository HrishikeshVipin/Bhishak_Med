# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5000/api
NEXT_PUBLIC_SOCKET_URL=http://localhost:5000

# Razorpay Configuration (Get from: https://dashboard.razorpay.com/app/website-app-settings/api-keys)
# Use TEST credentials for development
NEXT_PUBLIC_RAZORPAY_KEY_ID=rzp_test_YOUR_KEY_ID_HERE

# For production, use live keys:
# NEXT_PUBLIC_RAZORPAY_KEY_ID=rzp_live_YOUR_KEY_ID_HERE

# Feature Flags
# Enable/disable patient self-registration (OTP signup)
# Set to "true" to allow patient signup, "false" to disable
NEXT_PUBLIC_ENABLE_PATIENT_SIGNUP=false
